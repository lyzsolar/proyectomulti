<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" style="-fx-border-color: #B8B9BB; -fx-border-radius: 5; -fx-background-radius: 5;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lyzsolar.ajalcafe.controllers.EliminarMenuController">
    <children>
        <AnchorPane prefHeight="505.0" prefWidth="160.0" style="-fx-background-color: #41B8D5; -fx-border-color: #2B2B2B;">
            <children>
                <TextField alignment="CENTER" layoutX="2.0" layoutY="122.0" prefHeight="10.0" prefWidth="156.0" style="-fx-background-color: #41b8d5; -fx-border-color: #74371d;" text="Menú">
                    <font>
                        <Font size="15.0" />
                    </font>
                </TextField>
                <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="28.0" layoutY="14.0">
                    <image>
                        <Image url="@images/logo.png" />
                    </image>
                </ImageView>
            <ImageView fitHeight="150.0" fitWidth="150.0" layoutX="4.0" layoutY="170.0">
               <image>
                  <Image url="@images/delete.png" />
               </image>
            </ImageView>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="159.0" prefHeight="122.0" prefWidth="482.0" style="-fx-border-color: #8F8E8E;">
            <children>
                <Text fill="#0097b2" layoutX="91.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="¡ Eliminar Menú !" textAlignment="CENTER">
                    <font>
                        <Font name="Century Gothic" size="36.0" />
                    </font>
                </Text>
            </children>
        </AnchorPane>
        <ImageView fx:id="eliminarIcono" fitHeight="50.0" fitWidth="50.0" layoutX="254.0" layoutY="314.0" onMouseClicked="#OnMouseclickedEliminarIcono">
            <image>
                <Image url="@images/eliminar.png" />
            </image>
        </ImageView>
        <Text fill="#6b6969" layoutX="224.0" layoutY="176.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ingrese  nombre del menú que desea eliminar" textAlignment="CENTER">
            <font>
                <Font name="Century Gothic" size="16.0" />
            </font>
        </Text>
        <TextField fx:id="NombreText" alignment="CENTER" layoutX="251.0" layoutY="225.0" prefHeight="30.0" prefWidth="309.0" promptText="Nombre">
            <font>
                <Font name="Century Gothic" size="14.0" />
            </font>
            <effect>
                <DropShadow color="#1aa4c7" />
            </effect>
        </TextField>
        <Button fx:id="eliminarButton" alignment="CENTER" layoutX="231.0" layoutY="398.0" mnemonicParsing="false" onMouseClicked="#OnMouseclickedEliminarButton" prefHeight="30.0" prefWidth="101.0" style="-fx-background-color: #0097b2;" text="Eliminar" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Century Gothic" size="14.0" />
            </font>
        </Button>
        <Button fx:id="BuscarButton" alignment="CENTER" layoutX="370.0" layoutY="398.0" mnemonicParsing="false" onMouseClicked="#OnMouseclickedBuscarButton" prefHeight="30.0" prefWidth="101.0" style="-fx-background-color: #0097b2;" text="Buscar" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Century Gothic" size="14.0" />
            </font>
        </Button>
        <Button fx:id="regresarButton" alignment="CENTER" layoutX="500.0" layoutY="398.0" mnemonicParsing="false" onMouseClicked="#OnMouseclickedRegresarButton" prefHeight="30.0" prefWidth="101.0" style="-fx-background-color: #0097b2;" text="Regresar" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Century Gothic" size="14.0" />
            </font>
        </Button>
        <ImageView fx:id="regresarIcono" fitHeight="50.0" fitWidth="50.0" layoutX="526.0" layoutY="314.0" onMouseClicked="#OnMouseclickedRegresarIcono">
            <image>
                <Image url="@images/regreso.png" />
            </image>
        </ImageView>
      <ImageView fx:id="buscarIcono" fitHeight="50.0" fitWidth="50.0" layoutX="397.0" layoutY="314.0" onMouseClicked="#OnMouseclickedBuscarIcono">
         <image>
            <Image url="@images/Buscar.jpg" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
